{
    /* Global */
    /* Editor */
    "breadcrumbs.icons": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    // Files
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.empty": true,
        "**/.ipynb_checkpoints": true,
        "**/.mypy_cache": true,
        "**/__pycache__": true,
        "**/*.egg-info": true,
        "**/*.ionide": true
    },
    "files.associations": {
        "Launcher/config.json": "jsonc"
    },
    // Terminal
    "terminal.integrated.inheritEnv": true,
    /// Windows
    "terminal.integrated.shell.windows": null,
    "terminal.integrated.shellArgs.windows": [
        "-ExecutionPolicy",
        "ByPass",
        "-NoExit",
        "-Command",
        "& 'C:\\Users\\hellw\\Miniconda3\\shell\\condabin\\conda-hook.ps1' ; conda activate 'C:\\Users\\hellw\\Miniconda3'"
    ],
    "terminal.integrated.env.windows": {
        "MYPY_PATH": "C:\\Users\\hellw\\Miniconda3\\envs\\quantconnect-lean\\Scripts\\mypy.exe",
        "PYTHON": "C:\\Users\\hellw\\Miniconda3\\envs\\quantconnect-lean\\python.exe",
    },
    /// Linux
    "terminal.integrated.env.linux": {
        "MYPY_PATH": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/mypy",
        "PYTHON": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/python",
    },
    // Formatting
    "prettier.useEditorConfig": true,
    "prettier.disableLanguages": [
        "csharp",
        "fsharp",
        "json",
        "jsonc"
    ],
    "beautify.language": {
        "js": {
            "type": [
                "javascript",
                "json",
                "jsonc"
            ],
            "filename": [
                ".jshintrc",
                ".jsbeautifyrc"
            ]
        },
        "css": [
            "css",
            "less",
            "scss"
        ],
        "html": [
            "htm",
            "html"
        ]
    },
    /* Programming Languages */
    // JSON & JSONC
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.formatOnSave": true,
    },
    // C#
    "[csharp]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
    },
    "csharp.format.enable": true,
    "csharp.referencesCodeLens.enabled": true,
    "csharp.testsCodeLens.enabled": true,
    "csharp.unitTestDebuggingOptions": {},
    "csharp.suppressHiddenDiagnostics": true,
    // F#
    "FSharp.dotNetRoot": "/usr/share/dotnet/sdk",
    "FSharp.monoPath": "mono",
    "FSharp.workspacePath": "QuantConnect.Lean.sln",
    // Python
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "files.insertFinalNewline": true,
        "editor.formatOnSave": true
    },
    "python.autoComplete.addBrackets": true,
    "python.autoComplete.extraPaths": [
        "Algorithm.Python",
        "Algorithm.Python/Alphas",
        "Algorithm.Python/AltData",
        "Algorithm.Python/Benchmarks"
    ],
    "python.autoComplete.showAdvancedMembers": true,
    "python.pythonPath": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/python",
    "python.condaPath": "conda",
    /// Formatting
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Path": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/autopep8",
    /// Linting
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.linting.pylintPath": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/pylint",
    "python.linting.pycodestyleEnabled": false,
    "python.linting.mypyEnabled": true,
    "python.linting.mypyPath": "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/mypy",
    "python.linting.mypyArgs": [
        "--ignore-missing-imports",
        "--follow-imports=silent",
        "--python-executable",
        "/home/ahellwig/anaconda3/envs/quantconnect-lean/bin/python",
        "--config-file",
        "/home/ahellwig/Documents/IgadI/BusinessIntelligence/igadipromotions/setup.cfg"
    ],
    "python.linting.banditEnabled": false,
    "python.linting.pylamaEnabled": false,
    "python.linting.prospectorEnabled": false,
    "python.linting.pydocstyleEnabled": false,
}